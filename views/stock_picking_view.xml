<odoo>
    <record id="view_stock_move_tree_custom_gramaje" model="ir.ui.view">
        <field name="name">stock.move.tree.custom.gramaje</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_tree_without_package"/>
        <field name="arch" type="xml">
            <xpath expr="//tree" position="inside">
                <field name="gramaje" string="Gramaje"/>
            </xpath>
        </field>
    </record>
</odoo>
