<odoo>
    <record id="view_stock_picking_operations_tree" model="ir.ui.view">
        <field name="name">stock.move.tree.custom.gramaje</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='move_ids_without_package']//tree" position="inside">
                <field name="gramaje" string="Gramaje"/>
            </xpath>
        </field>
    </record>
</odoo>
